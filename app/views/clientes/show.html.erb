<div id="mostrar">
  <p id="notice"><%= notice %></p>
  
  <p>
    <strong>Nome:</strong>
    <%= @cliente.nome %>
  </p>
  
  <p>
    <strong>Cpf:</strong>
    <%= @cliente.cpf %>
  </p>
  
  <p>
    <strong>Endereco:</strong>
    <%= @cliente.endereco %>
  </p>
  
  <p>
    <strong>Telefone:</strong>
    <%= @cliente.telefone %>
  </p>
  
  <p>
    <strong>Email:</strong>
    <%= @cliente.email %>
